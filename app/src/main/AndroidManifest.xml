<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.davidwidjaya.moxvie">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectUAS">
        <activity android:name="com.davidwidjaya.moxvie.ui.search.SearchActivity"></activity>
        <activity android:name="com.davidwidjaya.moxvie.ui.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.davidwidjaya.moxvie.ui.favorite.DetailMovieFavoriteActivity" />
        <activity android:name="com.davidwidjaya.moxvie.ui.favorite.FavoriteActivity" />
        <activity android:name="com.davidwidjaya.moxvie.ui.detail.DetailMovieActivity" />
        <activity android:name="com.davidwidjaya.moxvie.ui.home.MainActivity"></activity>
    </application>

</manifest>